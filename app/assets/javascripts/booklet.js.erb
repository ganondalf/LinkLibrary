function addPage() {
  var linkUrl=document.location.href;
  var linkTitle=document.title;
  $.ajax({
    type: 'POST',
    url: 'http://localhost:3000/importlinks',
    dataType: 'json',
    crossDomain: true,
    data: {
      linkUrl: linkUrl,
      linkTitle: linkTitle,
      linkAnnotation: 'test'
    }
  }).done(function(response){
      alert('Your link has been added');
  });
}




