<div class='container'>
  <div class="sixteen columns" id="welcome">
    <h1>Welcome to LinkLibrary</h1>
      <button><%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %></button>
      <button id='login_button'><%= link_to 'Create a new account', new_user_path %>
    <div id='newUserFormDiv'>
      <%= render 'users/form' %>
    </div>
    <div id='returningUserFormDiv'>
      <%= render 'sessions/form' %>
    </div>
  </div>
</div>


